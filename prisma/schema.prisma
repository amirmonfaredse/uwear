datasource db{
  provider = "postgresql"
  url     =env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
  output ="../prisma/generated/generatePrisma"
}
model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
}
model Product{
  id Int @id @default(autoincrement())
  name String
  price Int
  createAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}